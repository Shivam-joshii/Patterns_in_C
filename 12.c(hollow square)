// HOLLOW SQUARE Of N size.
#include <stdio.h>

int main()
{
    int n;
    printf("Enter rows to print sqaure\n");
    scanf("%d",&n);
    int j, i;
    for (i = 1; i<=n; i++)
    {
        for (j= 1; j<=n; j++)
        {
            if (i==1 || j ==5)
            {
                printf("*");
            }
                else
                {
                   printf(" "); 
                }
            
        }
        printf("\n");
    }

    return 0;
}
